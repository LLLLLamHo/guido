{
  "name": "guido",
  "version": "4.0.0-beta1",
  "description": "基于webpack4的一键式开发工具，集成handlebars、雪碧图、SVG Sprite、自定义资源注入等方式",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuzucheFE/guido.git"
  },
  "bugs": {
    "url": "https://github.com/zuzucheFE/guido/issues"
  },
  "keywords": [
    "guido",
    "webpack",
    "framework",
    "tools",
    "frontend"
  ],
  "scripts": {
    "build:examples": "cd examples && node buildAll.js",
    "test": "./node_modules/.bin/mocha test/index.js",
    "test:cover": "rm -rf ./coverage && ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha test/index.js --report html -- -R spec",
    "travis:test": "npm run cover",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha test/index.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.11.5"
  },
  "bin": {
    "guido": "./bin/guido.js"
  },
  "main": "lib/guido.js",
  "dependencies": {
    "autoprefixer": "9.1.0",
    "babel-eslint": "8.2.6",
    "babel-loader": "8.0.0-beta.4",
    "babel-preset-zuzuche": "1.1.12",
    "bluebird": "3.5.1",
    "chalk": "2.4.1",
    "commander": "2.16.0",
    "css-loader": "1.0.0",
    "del": "3.0.0",
    "eslint": "5.2.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-react": "7.10.0",
    "extend": "3.0.2",
    "file-loader": "1.1.11",
    "glob": "7.1.2",
    "handlebars": "4.0.11",
    "handlebars-loader": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.3.1",
    "mini-css-extract-plugin": "0.4.1",
    "node-sass": "4.9.2",
    "postcss-loader": "2.1.6",
    "postcss-sprites": "4.2.1",
    "prompt": "1.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.21.0",
    "svg-sprite-loader": "3.9.0",
    "svg-url-loader": "2.3.2",
    "svgo": "1.0.5",
    "svgo-loader": "2.1.0",
    "text-table": "0.2.0",
    "url": "0.11.0",
    "url-loader": "1.0.1",
    "webpack": "4.16.4",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "4.1.4"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.2",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0"
  }
}
