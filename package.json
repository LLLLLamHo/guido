{
  "name": "guido",
  "version": "4.0.0-beta.3",
  "description": "基于webpack4的一键式开发工具，集成handlebars、雪碧图、SVG Sprite、自定义资源注入等方式",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuzucheFE/guido.git"
  },
  "bugs": {
    "url": "https://github.com/zuzucheFE/guido/issues"
  },
  "keywords": [
    "guido",
    "webpack",
    "framework",
    "tools",
    "frontend"
  ],
  "files": [
    "bin/",
    "lib/"
  ],
  "scripts": {
    "build:examples": "cd examples && node buildAll.js",
    "test": "jest",
    "prettier": "prettier --trailing-comma es5 --single-quote --write 'lib/*/*.js'"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.11.5"
  },
  "bin": {
    "guido": "./bin/guido.js"
  },
  "main": "lib/guido.js",
  "dependencies": {
    "@babel/core": "7.1.5",
    "address": "1.0.3",
    "autoprefixer": "9.3.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-preset-zuzuche": "2.2.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.1",
    "del": "3.0.0",
    "detect-port-alt": "1.1.6",
    "eslint": "5.9.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-react": "7.11.1",
    "extend": "3.0.2",
    "file-loader": "2.0.0",
    "filesize": "3.6.1",
    "friendly-errors-webpack-plugin": "1.7.0",
    "glob": "7.1.3",
    "gzip-size": "5.0.0",
    "handlebars": "4.0.12",
    "handlebars-loader": "1.7.0",
    "hard-source-webpack-plugin": "0.12.0",
    "html-webpack-plugin": "4.0.0-beta.2",
    "image-webpack-loader": "4.5.0",
    "mini-css-extract-plugin": "0.4.4",
    "node-sass": "4.10.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "postcss-safe-parser": "4.0.1",
    "postcss-sprites": "4.2.1",
    "recursive-readdir": "2.2.2",
    "sass-loader": "7.1.0",
    "strip-ansi": "5.0.0",
    "style-loader": "0.23.1",
    "svg-sprite-loader": "4.1.3",
    "svg-url-loader": "2.3.2",
    "svgo": "1.1.1",
    "svgo-loader": "2.2.0",
    "terser-webpack-plugin": "1.1.0",
    "text-table": "0.2.0",
    "url": "0.11.0",
    "url-loader": "1.1.2",
    "webpack": "4.25.1",
    "webpack-dev-server": "3.1.10",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.1.4",
    "webpackbar": "3.1.2"
  },
  "devDependencies": {
    "husky": "^1.1.3",
    "istanbul": "^0.4.5",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  }
}
